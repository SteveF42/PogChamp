{
  "name": "pogify",
  "version": "1.0.0",
  "description": "real time music player which people are allowed to control",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon ./server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "Poggers",
    "music",
    "spotify",
    "party",
    "player"
  ],
  "author": "steve flores",
  "license": "ISC",
  "dependencies": {
    "connect-mongo": "^3.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.11.16"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
