{
  "name": "pogify",
  "version": "1.0.0",
  "description": "real time music player which people are allowed to control",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon ./server/server.js",
    "client": "npm start --prefix client",
    "build-client": "cd client && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "Poggers",
    "music",
    "spotify",
    "party",
    "player"
  ],
  "author": "steve flores",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-proxy-middleware": "^1.0.6",
    "mongoose": "^5.11.18",
    "mongoose-type-url": "^2.1.0",
    "node-fetch": "^2.6.1",
    "random-key": "^0.3.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
